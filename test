
@Query("SELECT c FROM ContactPlatformLogin c WHERE c.appKey IN :appKeys AND LOWER(c.platformLogin) IN :lowerPlatformLogins")
List<ContactPlatformLogin> findContactsByAppKeyAndPlatformLoginIgnoreCase(@Param("appKeys") List<String> appKeys, @Param("lowerPlatformLogins") List<String> lowerPlatformLogins);
