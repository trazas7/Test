
Map<String, Contact> loginContactMap = new HashMap<>();
        for (Contact contact : contacts) {
            for (Map.Entry<String, String> entry : contact.getPlatformLogins().entrySet()) {
                if (entry.getKey().equals(request.getAppKey()) && lowerCaseLogins.contains(entry.getValue().toLowerCase())) {
                    loginContactMap.put(entry.getValue(), contact);
                }
            }
        }
